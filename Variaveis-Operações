    x = x + 1; 

    console.log(x);

let n1 = 15;
let n2 = 20;
let n3 = 10;

let soma = (n1 + n2) - n3;
    console.log(soma);

let subtr = (n2 - n1) + n3;
    console.log(subtr);

let multp = (n1 * n3);
    console.log(multp);

let divs = (n2 / n1);
    console.log(divs);

let potn = (n2 ^ 2);
    console.log(divs);


